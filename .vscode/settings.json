{
  "latex-workshop.latex.recipes": [
    {
      "name": "build",
      "tools": ["lualatex", "biber", "lualatex", "lualatex"]
    }
  ],
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bcf",
    "*.ilg",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.ist",
    "*.fls",
    "*.log",
    "*.nlo",
    "*.nls*",
    "*.run.xml",
    "*.fdb_latexmk"
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "lualatex",
      "command": "lualatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%.tex"
      ]
    },
    {
      "name": "biber",
      "command": "biber",
      "args": ["%DOCFILE%"]
    }
  ],
  "ltex.additionalRules.languageModel": "en",
  "ltex.language": "en-US",
  "ltex.enabled": true,
  "ltex.languageToolHttpServerUri": "",
  "latex-workshop.latex.autoBuild.run": "onSave"
}
