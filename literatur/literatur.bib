	keywords = {Expert recommendation, Fuzzy linguistic modeling, Knowledge management system, Text categorization, {EFExample}},
	file = {Li et al. - 2011 - An approach to expert recommendation based on fuzz.pdf:/Users/josch/Zotero/storage/XK8MP2NR/Li et al. - 2011 - An approach to expert recommendation based on fuzz.pdf:application/pdf;ScienceDirect Snapshot:/Users/josch/Zotero/storage/Y7XSVFF8/S0957417411000820.html:text/html},
